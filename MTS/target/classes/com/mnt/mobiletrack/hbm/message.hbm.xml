<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.mnt.mobiletrack.bean.Messages" table="Message">

<id name="messageid" column="Message_Id">
  <generator class="increment"></generator>
</id>
<property name="imienumber" column="IMIE_Number"></property>
<property name="messagenumber" column="MessageNumber"></property>
<property name="messagetype" column="MessageType"></property>
<property name="dt" column="DT"></property>
</class>
</hibernate-mapping>
