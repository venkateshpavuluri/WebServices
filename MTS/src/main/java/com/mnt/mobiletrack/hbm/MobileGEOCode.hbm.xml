<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.mnt.mobiletrack.bean.MobileGEOCode" table="MobileGEOCode">

<id name="mobileGEOCodeId" column="MobileGEOCodeId">
 <generator class="increment"></generator> 
</id>
<property name="iMIENumber" column="IMIE_Number"></property>
<property name="latitude" column="Latitude"></property>
<property name="longitude" column="Longitude"></property>
<property name="dT" column="DT"></property>
</class>

</hibernate-mapping>
